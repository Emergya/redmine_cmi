<tr>
  <th rowspan=7><%= l('cmi.label_planned') %></th>
  <td><%= l('cmi.label_JP') %></td>
  <% @names.each do |report| %>
      <td class="issue"><%= get_report_value(report, 'Esfuerzo actual JP') %> <%= l('cmi.label_hours') %></td>
